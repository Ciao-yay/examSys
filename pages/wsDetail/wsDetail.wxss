
.page{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}
/* .page view{
  margin: 15rpx 0;
} */

.head{
  height: 105rpx;
  width: 98%;
  display: flex;
  flex-direction: row;
  /* justify-content: center; */
  margin: 0 auto;
  font-size: 40rpx;
  line-height: 90rpx;
  text-align: center;
  color: rgb(0,154,214);
  border-top: 1rpx solid  rgb(204,204,204);
  border-bottom: 1rpx solid  rgb(204,204,204);
}
.headTittle{
  margin-left: 80rpx;
}
.logo{
  margin-left: 20rpx;
}
.head image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.information{
  background: rgb(80,183,193);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  color: white;
  width: 70%;
  box-shadow: 10rpx 10rpx 10rpx rgb(195,195,195);
  border-radius: 10rpx;
  padding: 8rpx 0;
}
.information view{
  margin: -4rpx 0;
  font-size: 30rpx;
}
.information view:first-child{
  font-size:40rpx
}
.body{
  margin: 20rpx;
  width: 95%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;

}
