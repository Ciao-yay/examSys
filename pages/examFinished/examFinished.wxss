/* pages/examFinshed/examFinshed.wxss */
.head{
  height: 105rpx;
  width: 98%;
  display: flex;
  flex-direction: row;
  /* justify-content: center; */
  margin: 0 auto;
  font-size: 40rpx;
  line-height: 90rpx;
  text-align: center;
  color: rgb(0,154,214);
  border-top: 1rpx solid  rgb(204,204,204);
  border-bottom: 1rpx solid  rgb(204,204,204);
}
.headTittle{
  margin-left: 60rpx;
}
.logo{
  margin-left: 20rpx;
}
.head image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.tittle{
  height: 100rpx;
  margin:30rpx 0 0 20rpx;
  font-size: 40rpx;

}
.score{
  font-size: 40pt;
  color: red;
  text-align: center;
  line-height: 400rpx;
  margin: 0 auto
}
.back {
  height: 90rpx;
  width: 40%;
  margin: 40rpx auto;
  color: white;
  background: rgb(0, 154, 214);
  box-shadow: 2px 2px 3px rgb(204, 204, 204);
  border-radius: 20rpx;
  text-align: center;
  font-size: 35rpx;
  line-height: 90rpx;
}